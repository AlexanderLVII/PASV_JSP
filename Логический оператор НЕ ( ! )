//1.	Новые значение двух величин 
function newValues(a, b){
return a !==b ? [a + b, a - b] : [0, 0]
}

//2.	Присвойте переменным новые значения
function newValues(a, b){
return a !== b ? a > b ? [a, a] : [b, b] : [0, 0]

